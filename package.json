    "npm-run-all": "^4.1.2",
    "numeral": "^2.0.6",
    "query-string-params": "^1.7.5",
    "react": "^16.4.1",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.5.0",
    "react-query-params": "^1.0.7",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
